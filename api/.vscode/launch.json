{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "node backend docker",
            "port": 9299,
            "restart": true,
            "remoteRoot": "/usr/src/app",
            "sourceMaps": true,
            "skipFiles": [
                "usr/src/app/node_modules/**/*/js",
                "<node_internals>/**"
            ]
        }
    ]
}
